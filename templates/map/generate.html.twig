<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <name>{{ map.name }}</name>
    <description></description>
    {% for name in names %} 
    <Placemark>
      <name>{{ name.name }}</name>
      <description>
       {{ name.description }}
      </description>
      <Point>
        <coordinates>{{ name.name.longitude }},{{ name.name.latitude }}</coordinates>
      </Point>
    </Placemark>
{% endfor %}
  </Document>
</kml>
